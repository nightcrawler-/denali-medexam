<p id="notice"><%= notice %></p>

<h1>Employee Examination Sessions</h1>

<table>
  <thead>
    <tr>
      <th>Bp</th>
      <th>Btw</th>
      <th>Fp</th>
      <th>Lmp</th>
      <th>Remarks</th>
      <th>Employee</th>
      <th>Session</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_examination_sessions.each do |employee_examination_session| %>
      <tr>
        <td><%= employee_examination_session.bp %></td>
        <td><%= employee_examination_session.btw %></td>
        <td><%= employee_examination_session.fp %></td>
        <td><%= employee_examination_session.lmp %></td>
        <td><%= employee_examination_session.remarks %></td>
        <td><%= employee_examination_session.employee_id %></td>
        <td><%= employee_examination_session.session_id %></td>
        <td><%= link_to 'Show', employee_examination_session %></td>
        <td><%= link_to 'Edit', edit_employee_examination_session_path(employee_examination_session) %></td>
        <td><%= link_to 'Destroy', employee_examination_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

